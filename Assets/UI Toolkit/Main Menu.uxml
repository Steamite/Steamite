                                <Bindings>
                                    <ui:DataBinding property="TextValue" data-source-path="Seed" data-source-type="MapGeneration, Toolkit" binding-mode="TwoWay" />
                                </Bindings>
                            </Params.StringGenerationParameter>
                            <Params.EnumGeneratioParameter label-text="Map Size" name="Map-Size" style="font-size: 37px; -unity-text-align: middle-left;">
                                <Bindings>
                                    <ui:DataBinding property="IntValue" data-source-path="MapSize" data-source-type="MapGeneration, Toolkit" binding-mode="ToSource" />
                                </Bindings>
                            </Params.EnumGeneratioParameter>
                            <ui:Label text="Veins" style="align-content: flex-end; font-size: 43px; height: 50px; margin-top: 20px; justify-content: flex-end;">
                                <ui:VisualElement style="flex-grow: 0; background-color: rgb(249, 233, 141); top: 5px; width: 80%; height: 3px; margin-top: 0; justify-content: space-between; align-self: flex-start; align-items: flex-end;" />
                            </ui:Label>
                            <Params.EnumGeneratioParameter label-text="Size" name="Size" style="font-size: 37px; -unity-text-align: middle-left; margin-top: 15px;">
                                <Bindings>
                                    <ui:DataBinding property="intValue" data-source-path="VeinSize" data-source-type="MapGeneration, Toolkit" binding-mode="ToSource" />
                                    <ui:DataBinding property="IntValue" data-source-path="VeinSize" data-source-type="MapGeneration, Toolkit" binding-mode="ToSource" />
                                </Bindings>
                            </Params.EnumGeneratioParameter>
                            <Params.EnumGeneratioParameter label-text="Richness" name="Richness" style="font-size: 37px; -unity-text-align: middle-left; margin-top: 10px;">
                                <Bindings>
                                    <ui:DataBinding property="intValue" data-source-path="VeinRichness" data-source-type="MapGeneration, Toolkit" binding-mode="ToSource" />
                                    <ui:DataBinding property="IntValue" data-source-path="VeinRichness" data-source-type="MapGeneration, Toolkit" binding-mode="ToSource" />
                                </Bindings>
                            </Params.EnumGeneratioParameter>
                            <Params.EnumGeneratioParameter label-text="Number" name="Number" style="font-size: 37px; -unity-text-align: middle-left; margin-top: 10px;">
                                <Bindings>
                                    <ui:DataBinding property="intValue" data-source-path="VeinCount" data-source-type="MapGeneration, Toolkit" binding-mode="ToSource" />
                                    <ui:DataBinding property="IntValue" data-source-path="VeinCount" data-source-type="MapGeneration, Toolkit" binding-mode="ToSource" />
                                </Bindings>
                            </Params.EnumGeneratioParameter>
                        </ui:ScrollView>
                        <ui:Label text="No options&#10;available" name="PlaceHolder" style="-unity-text-align: middle-center; flex-grow: 1; flex-shrink: 1; font-size: 80px; display: flex;" />
                    </ui:VisualElement>
                    <ui:VisualElement name="Right-Bar" style="flex-grow: 1; flex-shrink: 1; margin-right: 1%;">
                        <ui:VisualElement name="VisualElement" style="flex-grow: 1; height: 80%; visibility: visible;">
                            <ui:Label text="Predefined" name="Label" style="font-size: 50px; flex-grow: 0; height: 10%; align-self: flex-start; margin-left: 0; margin-right: 0;" />
                            <ui:Label text="Label" name="Label" style="flex-grow: 1; height: 10%; max-height: 20%; padding-bottom: 0; margin-bottom: 0; align-self: auto; margin-left: 0; margin-right: 0; padding-left: 0; padding-right: 0; font-size: 20px;" />
                            <ui:VisualElement name="VisualElement" style="flex-grow: 1; margin-bottom: 0; height: 100%; background-image: url(&quot;project://database/Assets/Sprites/Icons/Gear.png?fileID=2800000&amp;guid=fd35f4632523ad441b9cd8631eecdbc2&amp;type=3#Gear&quot;); border-top-left-radius: 1px; border-top-right-radius: 1px; border-bottom-right-radius: 1px; border-bottom-left-radius: 1px; border-top-width: 0; border-right-width: 0; border-bottom-width: 0; border-left-width: 0; border-left-color: rgb(253, 220, 35); border-right-color: rgb(253, 220, 35); border-top-color: rgb(253, 220, 35); border-bottom-color: rgb(253, 220, 35); width: 100%; margin-top: 5%; align-self: auto; align-content: auto; align-items: auto; padding-bottom: 0;" />
                            <ui:VisualElement style="flex-grow: 0; max-height: none; height: 200px; align-self: center; align-items: flex-start; align-content: auto; justify-content: flex-start; margin-left: 30px; margin-right: 30px; margin-top: 20px; padding-top: 10px; padding-bottom: 0; margin-bottom: 0; width: 85%; max-width: 85%; min-width: 85%;">
                                <ui:Label text="world name" style="flex-grow: 0; height: 10px; margin-bottom: 5px; max-height: 20px; align-self: center; font-size: 39px; -unity-text-align: middle-left; align-content: auto; align-items: auto; flex-shrink: 0;" />
                                <ui:TextField placeholder-text="World name" hide-placeholder-on-focus="false" name="World-Name" max-length="24" style="width: auto; max-width: 100%; min-width: 100%; overflow: hidden; max-height: 70%; margin-top: 20px; margin-bottom: 0; font-size: 16px; -unity-text-align: upper-left;">
                                    <Bindings>
                                        <ui:DataBinding property="value" data-source-path="WorldName" data-source-type="NewGameWindow, Toolkit" binding-mode="ToSource" />
                                    </Bindings>
                                </ui:TextField>
                            </ui:VisualElement>
                        </ui:VisualElement>
                        <ui:VisualElement style="flex-grow: 1; margin-right: 0;">
