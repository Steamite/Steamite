                        <ui:VisualElement name="VisualElement" style="flex-grow: 0; width: 5px; height: 90%; background-color: rgb(249, 233, 141); position: relative; left: 20px; align-self: center; display: flex; top: -10px; margin-top: 25%;" />
                    </ui:VisualElement>
