<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <ui:Template name="Load-Menu" src="project://database/Assets/UI%20Toolkit/Load-Menu.uxml?fileID=9197481963319205126&amp;guid=87550cde01cc7fa4a83489895c6bf595&amp;type=3#Load-Menu" />
    <Style src="project://database/Assets/UI%20Toolkit/Styles/Button.uss?fileID=7433441132597879392&amp;guid=9c069573d2a1ca645bcaf03cbee89a38&amp;type=3#Button" />
    <Style src="project://database/Assets/UI%20Toolkit/Styles/Window.uss?fileID=7433441132597879392&amp;guid=a52c4c079ba57c5419681c2f3af6656f&amp;type=3#Window" />
    <Style src="project://database/Assets/UI%20Toolkit/Styles/Save%20window.uss?fileID=7433441132597879392&amp;guid=eca73d8699a9e1c41a8dd8849b9c0de9&amp;type=3#Save window" />
    <ui:VisualElement name="VisualElement" class="Empty" style="-unity-font-definition: resource(&apos;Fonts &amp; Materials/custom fonts/assets/Alpha-Slab-One&apos;); font-size: 50px;">
        <ui:VisualElement name="Canvas" style="flex-grow: 1; padding-top: 0; padding-right: 0; padding-bottom: 0; padding-left: 0; background-color: rgb(70, 49, 27);">
            <ui:VisualElement name="Header" style="flex-grow: 1; -unity-text-align: middle-center; align-self: flex-start; justify-content: center; height: 304px; min-height: 30%; max-height: 30%; margin-top: 2%; background-image: url(&quot;project://database/Assets/Sprites/Header.png?fileID=2800000&amp;guid=c36688c0ec6f10140960af633a7f009f&amp;type=3#Header&quot;); background-size: 70% 90%; border-left-width: 0; width: 100%;" />
            <ui:VisualElement name="Menu" style="flex-grow: 1; max-width: 40%; align-self: flex-start; left: 30%; right: 35px; justify-content: flex-start; align-content: auto; max-height: 70%; align-items: auto; min-height: auto;">
                <ui:VisualElement name="Menu" style="flex-grow: 1; height: 20%; min-height: auto; max-height: 100%; align-content: center; align-items: center; justify-content: flex-start; margin-top: 0; margin-left: 5%; margin-right: 5%; margin-bottom: 0; align-self: center; padding-top: 2%;">
                    <ui:Button text="Continue" name="Continue-Button" class="disabled-button" style="width: 100%; height: 18%; background-image: url(&quot;project://database/Assets/Sprites/Textures/Button.png?fileID=2800000&amp;guid=3e672b094164b1549a98c05385591f88&amp;type=3#Button&quot;); background-color: rgba(255, 255, 255, 0); color: rgb(249, 233, 141); font-size: 90px; margin-top: 1%; padding-top: 0; padding-right: 0; padding-left: 0; padding-bottom: 1%; margin-left: 0; margin-right: 0; margin-bottom: 0; border-top-color: rgba(255, 255, 255, 0); border-left-color: rgba(255, 255, 255, 0); border-right-color: rgba(255, 255, 255, 0); border-bottom-color: rgba(255, 255, 255, 0);" />
                    <ui:Button text="New Game" name="New-Game-Button" selectable="false" class="main-button" style="margin-top: 1%;" />
                    <ui:Button text="Load Game" name="Load-Game-Button" class="disabled-button" style="width: 100%; height: 18%; background-image: url(&quot;project://database/Assets/Sprites/Textures/Button.png?fileID=2800000&amp;guid=3e672b094164b1549a98c05385591f88&amp;type=3#Button&quot;); background-color: rgba(255, 255, 255, 0); color: rgb(249, 233, 141); margin-top: 1%; padding-top: 0; padding-right: 0; padding-left: 0; margin-left: 0; margin-right: 0; margin-bottom: 0; border-top-color: rgba(255, 255, 255, 0); border-left-color: rgba(255, 255, 255, 0); border-right-color: rgba(255, 255, 255, 0); border-bottom-color: rgba(255, 255, 255, 0); font-size: 90px;" />
                    <ui:Button text="Settings" name="Settings-Button" class="main-button" style="margin-top: 1%; margin-right: 0; margin-bottom: 0; margin-left: 0;" />
                    <ui:Button text="Exit" name="Exit-Button" class="main-button" style="margin-top: 1%; margin-right: 0; margin-bottom: 0; margin-left: 0;" />
                </ui:VisualElement>
