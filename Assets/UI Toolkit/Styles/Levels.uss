    max-height: 5%;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    -unity-text-align: middle-left;
}

#Level-View > #Body {
    flex-grow: 1;
    justify-content: space-between;
    margin-top: 0;
    margin-bottom: 0%;
    margin-right: 5%;
    margin-left: 5%;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
}

#Cost #ResText Label {
    font-size: 35px;
}
