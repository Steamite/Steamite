#Level-View {
    flex-grow: 1;
    justify-content: flex-start;
    margin-top: 5%;
    margin-bottom: 0;
    margin-right: 5%;
    margin-left: 2.5%;
    transition-duration: 0.2s;
    align-self: stretch;
    align-content: stretch;
}

#Level-View > #Header {
    flex-grow: 1;
    justify-content: space-between;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    max-height: 5%;
    -unity-text-align: middle-left;
}

#Level-View > #State {
    flex-grow: 1;
    justify-content: space-between;
    margin-top: 5%;
    margin-bottom: 5%;
    margin-right: 5%;
    margin-left: 5%;
    max-height: 5%;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    -unity-text-align: middle-left;
}

#Level-View > #Body {
    flex-grow: 1;
    justify-content: space-between;
    margin-top: 0;
    margin-bottom: 0%;
    margin-right: 5%;
    margin-left: 5%;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
}

#Cost #ResText Label {
    font-size: 35px;
}
