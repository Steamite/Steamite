#ResText {
    flex-direction: row;
    padding-top: 5%;
    padding-bottom: 5%;
    padding-right: 5%;
    max-width: 100%;
    flex-grow: 1;
    justify-content: center;
    padding-left: 10%;
}

#ResText > #Value {
    -unity-text-align: upper-right;
    font-size: 38px;
    margin-right: 2%;
    padding-right: 0;
    padding-bottom: 0;
    padding-top: 0;
    padding-left: 0;
    margin-left: 5%;
    flex-grow: 0;
    margin-bottom: 0;
    margin-top: 0;
    max-width: none;
}

#ResText > #Icon {
    flex-grow: 1;
    max-width: 50px;
    max-height: 50px;
    background-image: url("project://database/Assets/Sprites/Icons/Info%20window/ResIcon@4x.png?fileID=2800000&guid=9c948cd08b1522c4a95a3a66fe6242d4&type=3#ResIcon@4x");
    justify-content: flex-start;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    flex-shrink: 0;
    min-width: 50px;
    min-height: 50px;
}
