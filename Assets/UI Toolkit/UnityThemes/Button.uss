}

.save-radio-button-selected {
    font-size: 40%;
    height: 8%;
    margin-top: 0;
    padding-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    background-color: rgb(73, 121, 107);
    color: rgb(193, 68, 14);
