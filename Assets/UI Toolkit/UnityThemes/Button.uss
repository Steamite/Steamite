.main-button {
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 1%;
    padding-left: 0;
    margin-top: 5%;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    font-size: 100px;
    min-width: 100%;
    background-image: url("project://database/Assets/Sprites/Button.png?fileID=649223268058989734&guid=3e672b094164b1549a98c05385591f88&type=3#Button_0");
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-color: rgba(0, 0, 0, 0);
    background-size: 100% 100%;
    height: 18%;
    width: 100%;
    color: rgb(249, 233, 141);
    -unity-text-align: lower-center;
    border-left-color: rgba(255, 255, 255, 0);
    border-right-color: rgba(255, 255, 255, 0);
    border-top-color: rgba(255, 255, 255, 0);
    border-bottom-color: rgba(255, 255, 255, 0);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
}

.main-button:hover {
    scale: 1.05 1.05;
    transition-duration: 0.1s;
    transition-timing-function: ease-out;
    transition-property: scale;
    -unity-background-image-tint-color: rgb(180, 180, 180);
}

.main-button:active {
    -unity-background-image-tint-color: rgb(166, 154, 154);
}

.world-radio-button {
    font-size: 40%;
    height: 8%;
    -unity-font-definition: resource('Fonts & Materials/custom fonts/assets/Roadstore');
    background-color: rgba(0, 0, 0, 0);
    background-image: url("project://database/Assets/Sprites/Button.png?fileID=2800000&guid=3e672b094164b1549a98c05385591f88&type=3#Button");
    -unity-background-image-tint-color: rgb(123, 123, 123);
    transition-duration: 0.2s;
    transition-timing-function: linear;
}

.world-radio-button-selected {
    font-size: 40%;
    height: 8%;
    margin-top: 0;
    padding-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    background-color: rgba(0, 0, 0, 0);
    color: rgb(250, 88, 18);
    -unity-font-definition: resource('Fonts & Materials/custom fonts/assets/Roadstore');
    background-image: url("project://database/Assets/Sprites/Button.png?fileID=2800000&guid=3e672b094164b1549a98c05385591f88&type=3#Button");
    -unity-background-image-tint-color: rgb(212, 212, 212);
}

.save-radio-button {
    font-size: 40%;
    height: 8%;
    margin-top: 0;
    padding-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    -unity-font-definition: resource('Fonts & Materials/custom fonts/assets/Alpha-Slab-One');
    background-color: rgb(74, 74, 74);
